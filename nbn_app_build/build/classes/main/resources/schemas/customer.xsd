    <xsd:complexType name="Customer" abstract="false">
        <xsd:annotation>
            <xsd:documentation xml:lang="en-AU"></xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="common:PartyRole">
                <xsd:sequence>
                    <xsd:element name="customerStatus" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU"></xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="customerRank" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension Gold Valued, High Value, Low
                                Value
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="customerType" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension Retail or Wholsale.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="salesClass" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="isListingRequired" type="xsd:boolean" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU"></xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="privacyType" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension - To indicate if it is secret
                                account i.e. can only be accessed by authorized persion. If it is at customer level then
                                all accounts under that customer will be secret accounts
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="customerUID" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension - UID used in the customer
                                master data management system
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="customerExternalIdentifier" type="xsd:string" minOccurs="0"
                        maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en-AU">Optus Extension - It could be email address or
                                passport number
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="creditCardsOwnedByCustomer" type="customer:CreditCardPM" minOccurs="0"
                        maxOccurs="unbounded">
                    </xsd:element>
                    <xsd:element name="customerQuote" type="customer:CustomerQuote" minOccurs="0" maxOccurs="unbounded">
                    </xsd:element>
                    <xsd:element name="loyaltyAccount" type="customer:LoyaltyAccount" minOccurs="0"
                        maxOccurs="unbounded">
                    </xsd:element>
                    <xsd:element name="customerCreditProfile" type="customer:CustomerCreditProfile"
                        minOccurs="0" maxOccurs="unbounded">
                    </xsd:element>
                    <xsd:element name="marketSegment" type="common:MarketSegment" minOccurs="0" maxOccurs="1">
                    </xsd:element>
                    <xsd:element name="customerAccount" type="customer:CustomerAccount" minOccurs="0"
                        maxOccurs="unbounded">
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
